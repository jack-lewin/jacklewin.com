{
  "name": "jacklewin.com",
  "description": "Source for jacklewin.com, created using Gatsby and Netlify CMS",
  "version": "1.0.0",
  "author": "Jack Lewin",
  "scripts": {
    "build": "yarn clean && gatsby build",
    "clean": "rimraf .cache public",
    "dev": "yarn clean && gatsby develop",
    "format": "eslint \"{gatsby-*.js,src/**/*.js}\" --fix",
    "lint": "eslint \"{gatsby-*.js,src/**/*.js}\"",
    "serve": "gatsby serve",
    "start": "yarn build && yarn serve",
    "test": "NODE_ENV=test jest",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "gatsby": "^2.0.66",
    "gatsby-image": "^2.0.23",
    "gatsby-plugin-netlify": "^2.0.6",
    "gatsby-plugin-netlify-cms": "^3.0.9",
    "gatsby-plugin-purgecss": "^2.4.0",
    "gatsby-plugin-react-helmet": "^3.0.4",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sass": "^2.0.7",
    "gatsby-plugin-sharp": "^2.0.15",
    "gatsby-plugin-sitemap": "^2.0.3",
    "gatsby-remark-copy-linked-files": "^2.0.7",
    "gatsby-remark-images": "^3.0.1",
    "gatsby-remark-relative-images": "^0.2.0",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-transformer-remark": "^2.1.15",
    "gatsby-transformer-sharp": "^2.1.9",
    "netlify-cms": "^2.3.1",
    "node-sass": "^4.11.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.4.1",
    "react-helmet": "^5.2.0",
    "react-test-renderer": "^16.7.0"
  },
  "devDependencies": {
    "@jack-lewin/eslint-config": "^1.0.1",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "jest-prop-type-error": "^1.1.0",
    "prettier": "^1.15.3",
    "rimraf": "^2.6.2"
  },
  "license": "MIT",
  "resolutions": {
    "babel-core": "7.0.0-bridge.0"
  },
  "jest": {
    "setupFiles": [
      "jest-prop-type-error"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>src/setupTests.js",
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    }
  }
}
